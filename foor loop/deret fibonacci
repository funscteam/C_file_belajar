#include <iostream>
using namespace std;
int main () {
    
    int n, first = 0, second = 1, next;
    
    cout << "Masukan jumlah elemen deret Fibonacci :";
    cin >> n;
    
    cout << "Deret fibonacci ";
    
    for (int i = 0; i < n; ++i) {
        cout << first << " ";
        next = first + second;
        first = second;
        second = next;
    }
    return 0;
}
