#include <iostream>
using namespace std;
int main () {
    
    
    for (int i = 1; i <= 5; i++) {
        if ( i % 2 == 0) {
            cout << "Iterasi ke-" << i << ": genap " << endl;
        } else {
            cout << "Iterasi ke-" << i << ": ganjil" << endl;
        }
    }
    return 0;
}
