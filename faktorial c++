#include <iostream>
using namespace std;
int main () {
    int angka;
    long long faktorial = 1;
    
    cout << "Masukan angka:";
    cin >> angka;
    
    for(int i = 1; i <= angka; ++i) {
        faktorial *= i;
    } 
    
    cout << "Faktorial dari " << angka << " adalah : " << faktorial << endl;
    
    return 0;
}
